
{{#if news}}
    <section class="card rightbox">
        <h2>Publicity, Discussion and Events</h2>
        <div class="info">
            <dl>
            {{#each news}}
                <dt><a href="{{url}}" rel="nofollow">{{title}}</a></dt>
                <dd>{{date}} {{source}} </dd>
            {{/each}}
            </dl>
        </div>
    </section>
{{/if}}